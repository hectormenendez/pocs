{
	"parser": "babel-eslint",
	"env":{
		"browser" : false,
		"node"    : true
	},
	"rules":{
		"quotes"               : [2, "single"],
		"no-unused-vars"       : [1],
		"no-unused-expressions": [1],
		"no-use-before-define" : [1],
		"comma-spacing"        : [1],
		"dot-notation"         : [1],
		"no-multi-spaces"      : [0],
		"curly"                : [0],
		"eol-last"             : [0],
		"key-spacing"          : [0],
		"new-cap"              : [0],
		"no-underscore-dangle" : [0],
		"no-process-exit"      : [0],
		"no-new-wrappers"      : [0],
		"camelcase"            : [0]
	}
}