{
    "parserOptions":{
        "ecmaVersion"  : 6,
        "ecmaFeatures" : {
        }
    },
    "env":{
        "browser" : true,
        "node"    : true
    },
    "rules":{
        "quotes"               : [2, "single"],
        "no-unused-vars"       : [0],
        "no-unused-expressions": [1],
        "no-use-before-define" : [0],
        "comma-spacing"        : [0],
        "dot-notation"         : [1],
        "no-multi-spaces"      : [0],
        "curly"                : [0],
        "eol-last"             : [0],
        "key-spacing"          : [0],
        "new-cap"              : [0],
        "no-underscore-dangle" : [0],
        "no-process-exit"      : [0],
        "no-new-wrappers"      : [0],
        "camelcase"            : [0]
    }
}
