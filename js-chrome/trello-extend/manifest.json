{
	"name"        : "YappTrello",
	"version"     : "1.0",
	"description" : "Trello extensions for Yapp Development.",

	"content_scripts": [{
		"css"     : ["yapptrello.css"],
		"js"      : ["jquery-2.1.1.min.js","yapptrello.js", "yappevents.js"],
		"matches" : ["https://trello.com/*"],
		"run_at"  : "document_end"
	}],

   "icons": {
      "128": "icon.jpg"
   },

   "permissions"      : ["storage"],
   "manifest_version" : 2
}
