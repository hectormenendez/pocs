{
    "name": "@etor.dev/back-core",
    "version": "0.0.1",
    "private": true,
    "type": "module",
    "engines": {
        "node": ">=14.19.0",
        "npm": ">=9.2.0",
        "deno": ">=1.29.0"
    },
    "os": [
        "darwin",
        "linux",
        "windows"
    ],
    "config": {
        "aws_name": "core",
        "deno_main": "./main.ts",
        "deno_flags": "--unstable --allow-net --allow-read --allow-write --allow-env --import-map=../imports.json"
    },
    "scripts": {
        "start": "deno run $npm_package_config_deno_flags $npm_package_config_deno_main",
        "build": "docker build -t ${npm_package_config_aws_repo}:${npm_package_config_aws_name}-v${npm_package_version} .",
        "push": "docker push ${npm_package_config_aws_repo}:${npm_package_config_aws_name}-v${npm_package_version}",
        "deploy": "npm run build && npm run push"
    },
    "dependencies": {},
    "devDependencies": {}
}
